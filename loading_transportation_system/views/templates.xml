            </t>

        </template>

        <template id="loading_transportation_system.lead_temp">
            <t t-call="portal.portal_layout">
                <div class="my_leads"/>
                <div class="create_lead"/>
            </t>

        </template>




        <template id="loading_transportation_system.transporter_registration">
            <t t-call="portal.portal_layout">

                <div class="transporter_registrationform"></div>
            </t>

        </template>

        <template id="loading_transportation_system.display_driver">
            <t t-call="portal.portal_layout">

                <!-- <div class="transporter_temp"></div> -->
                <!-- <div class="vehicle_temp"></div> -->
                <!-- <div class="create_vehicle"></div> -->
                <div class="driver_temp"></div>
                <div class="create_driver"></div>
            </t>

        </template>

        <template id="loading_transportation_system.display_vehicle">
            <t t-call="portal.portal_layout">

                <!-- <div class="transporter_temp"></div> -->
                <div class="vehicle_temp"></div>
                <div class="create_vehicle"></div>
                <!-- <div class="my_attribute"></div> -->
                <!-- <div class="create_driver"></div> -->
            </t>

        </template>




        <template id="loading_transportation_system.customer_registration">
            <t t-call="portal.portal_layout">

                <div class="customer_registrationform"></div>
                
            </t>
        </template>

