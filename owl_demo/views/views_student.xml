<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_frontend" name="assets" inherit_id="web.assets_frontend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/owl_demo/static/src/js/owl_component.js"></script>
                <script type="text/javascript" src="/owl_demo/static/src/js/owl_dynamic_component.js"></script>
                <script type="text/javascript" src="/owl_demo/static/src/js/ragistartion.js"></script>
                <script type="text/javascript" src="/owl_demo/static/src/js/AddStudent.js"></script>
                <script type="text/javascript" src="/owl_demo/static/src/js/add_cource.js"></script>
                <link href="/owl_demo/static/src/scss/app.scss" rel="stylesheet" type="text/scss" />
                <link href="/owl_demo/static/src/scss/rag.scss" rel="stylesheet" type="text/scss" />
                <script type="text/javascript" src="/owl_demo/static/src/js/studentlist.js"></script>
                <script type="text/javascript" src="/owl_demo/static/src/js/menu.js"></script>
                <script type="text/javascript" src="/owl_demo/static/src/js/payment.js"></script>
                <script type="text/javascript" src="/owl_demo/static/src/js/accepted.js"></script>
            </xpath>
        </template>
        <template id="owl_demo.student">
            <t t-call="portal.portal_layout">
                <div class="payment_com"></div>
            </t>
        </template>
        <template id="student_item" name="second" inherit_id="portal.portal_breadcrumbs" priority="2">
            <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
                <ul>
                    <li>
                        <a href="/owl_demo_student">Pyment</a>
                    </li>
                </ul>
                <ul>
                    <li>
                        <a href="#">Payment details</a>
                    </li>
                </ul>
            </xpath>
        </template>
    </data>
</odoo>