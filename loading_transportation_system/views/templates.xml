                <script type="text/javascript" src="/loading_transportation_system/static/src/js/user.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/lead.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/createlead.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/transporter.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/customer_regi.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/transporter_regi.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/vehicle.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/driver.js"></script>
                <!-- <script type="text/javascript" src="/loading_transportation_system/static/src/js/addproduct.js"></script> -->
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/orders_detail.js"></script>
                <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/cust_vehicle.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/order.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/menu.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/menu2.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/createvehicle.js"></script>
                <script type="text/javascript" src="/loading_transportation_system/static/src/js/createdriver.js"></script>
            </xpath>
        </template>
       
       <template id="loading_transportation_system.menu_item2">
            <t t-call="portal.portal_layout">
                <div class="menu2_temp"/>
                <div class="component_view"/>
            </t>
        </template>

        <template id="loading_transportation_system.transporter_item">
            <t t-call="portal.portal_layout">
                <div class="trans_item"/>
                <div class="component_view"/>
            </t>
        </template>

        <template id="loading_transportation_system.orders_template">
            <t t-call="portal.portal_layout">
                <div class="order_List">
                </div>
                <div class="orders_details">
                </div>
            </t>
        </template>

        <template id="loading_transportation_system.display_orders">
            <t t-call="portal.portal_layout">
                <div class="create_order">
                </div>
                <div class="orderdisplay"></div>
            </t>
        </template>

        <template id="loading_transportation_system.display_orders2">
            <t t-call="portal.portal_layout">
                <!-- <div class="create_order">
                </div> -->
                <div class="orderdisplay"></div>
            </t>
        </template>




<!--         <template id="loading_transportation_system.addmyproduct">
            <t t-call="portal.portal_layout">
                <div class="addproductdata">
                </div>
            </t>
        </template> -->

        <template id="loading_transportation_system.demo_template">
            <t t-call="portal.portal_layout">
                <!-- <div class="my_user_component"/> -->

                <div class="transporter_temp"></div>
            </t>

        </template>

        <template id="loading_transportation_system.demo_template2">
