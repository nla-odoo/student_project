    <record id="config_group_uom" model="res.config.settings">
        <field name="group_uom">True</field>
    </record>

    <record id="rental_price_mustang" model="rental.pricing">
        <field name="duration">1</field>
        <!-- <field name="unit">km</field> -->
        <field name="price">20.00</field>
    </record>

    <record id="project_mustang" model="project.project">
        <field name="name">Mustang</field>
        <field name="allow_timesheets">True</field>
    </record>

    <record id="product_attribute_seats" model="product.attribute">
        <field name="name">Seats</field>
    </record>

    <record id="attribute_value_5_seater" model="product.attribute.value">
        <field name="name">5-seater</field>
        <field name="attribute_id" ref="product_attribute_seats"/>
    </record>

    <record id="attribute_value_7_seater" model="product.attribute.value">
        <field name="name">7-seater</field>
        <field name="attribute_id" ref="product_attribute_seats"/>
    </record>

	<record id="product_mustang" model="product.template">
		<field name="name">Mustang</field>
		<field name="sale_ok">False</field>
		<field name="type">service</field>
		<field name="purchase_ok">False</field>
		<field name="rent_ok">True</field>
		<field name="default_code">RJ-13/DLC/12/ 222222</field>
		<field name="standard_price">10</field>
		<field name="list_price">13</field>
		<field name="service_tracking">task_global_project</field>
		<field name="project_id" ref="project_mustang"/>
		<field name="rental_pricing_ids" eval="[(6,0,[ref('rental_price_mustang')])]"/>
		<field name="extra_hourly">200.00</field>
		<field name="extra_daily">5000.00</field>
	</record>

    <record id="product_template_attribute_line_1" model="product.template.attribute.line">
        <field name="product_tmpl_id" ref="product_mustang"/>
        <field name="attribute_id" ref="product_attribute_seats"/>
        <field name="value_ids" eval="[(6,0,[ref('attribute_value_5_seater'), ref('attribute_value_7_seater')])]"/>
    </record>

