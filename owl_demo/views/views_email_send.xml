<?xml version="1.0" encoding="UTF-8"?>

<odoo>
<record id="email_send_student" model="mail.template">
    <field name="name">Student</field>
    <field name="email_from">${object.email_from}</field>
    <field name="email_to">${object.email_to}</field>
    <field name="subject">mail id</field>
    <!-- <field name="partner_to">${object.partner_id.id}</field> -->
    <field name="model_id" ref="model_res_users" />
    <field name="body_html" type="xml">

        <h1>Your Email ID: ${object.email}</h1>
        <h1>Your Password: ${object.pass}</h1>

    </field>
</record>
</odoo>